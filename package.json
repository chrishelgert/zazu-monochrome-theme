{
  "name": "zazu-monochrome-theme",
  "version": "1.0.0",
  "description": "A monochrome theme for Zazu.",
  "main": "dist/main.css",
  "scripts": {
    "build": "postcss -u postcss-cssnext -o dist/main.css -m dist/main.css.map src/main.css",
    "test": "stylelint src/*.css",
    "precommit": "npm test && npm run build"
  },
  "author": "Chris Helgert",
  "license": "MIT",
  "keywords": [
    "theme",
    "zazu"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/chrishelgert/zazu-monochrome-theme.git"
  },
  "bugs": {
    "url": "https://github.com/chrishelgert/zazu-monochrome-theme/issues"
  },
  "devDependencies": {
    "husky": "^0.14.0",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.0",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0"
  }
}
